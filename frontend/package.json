{
  "name": "infokes-frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "all:fix": "bun run prettier:fix && bun run eslint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest --reporter=verbose",
    "test:watch": "vitest --reporter=verbose --watch",
    "test:coverage": "vitest --reporter=html --coverage",
    "test:coverage:watch": "vitest --reporter=html --coverage --watch",
    "test:browser": "vitest --reporter=html --coverage --browser",
    "test:browser:watch": "vitest --reporter=html --coverage --browser --watch",
    "test:browser:debug": "vitest --reporter=html --coverage --browser --watch --debug",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/ui": "^3.3.2",
    "@tailwindcss/vite": "^4.1.12",
    "nuxt": "^4.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4",
    "@tsconfig/recommended": "^1.0.10",
    "browserslist-useragent-regexp": "^4.1.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "tailwindcss": "^4.1.12"
  }
}
